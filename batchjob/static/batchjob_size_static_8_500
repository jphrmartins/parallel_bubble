#SBATCH -o /home/pp03011/openmp/parallel_bubble/exit/%x.%j_static_8_500.out -e /home/pp03011/openmp/parallel_bubble/exit/%x.%j_static_8_500.err # formato do nome do arquivo de saÃ­da
#SBATCH -D /home/pp03011/openmp/parallel_bubble # diretÃ³rio
